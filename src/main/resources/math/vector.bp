public struct vector3<T>{
    T x,y,z

    public void plusAssign(vector3<T> other){
        x += other.x
        y += other.y
        z += other.z
    }

    public void minusAssign(vector3<T> other){
        x -= other.x
        y -= other.y
        z -= other.z
    }

    public void timesAssign(T other){
        x *= x
        y *= y
        z *= z
    }

    public void divAssign(T other){
        x /= x
        y /= y
        z /= z
    }
}
public typedef vector3<int> vector3Int